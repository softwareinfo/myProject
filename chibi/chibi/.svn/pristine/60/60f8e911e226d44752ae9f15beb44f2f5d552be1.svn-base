	<script>		
		window.onload = function(){
			var audio = document.getElementById('audio');
			var btn = document.getElementById('audio_btn');
			var canvas = document.getElementsByTagName("canvas");
			var timer = null;
			var isClick = false;

			btn.onclick = function(){
				if(isClick){
					audio.play();
					isClick = false;
				}else{
					audio.pause();
					isClick = true;
				}				
			}

			//tag标签的落下
			new WOW().init();

			//定义画布大小
			var winH = document.body.clientWidth;
			for(var i = 0;i < canvas.length; i++){
				canvas[i].width = winH;
				canvas[i].height = winH;
			}
		};
	</script>
